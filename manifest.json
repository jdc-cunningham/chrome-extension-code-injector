{
    "name": "Basic custom code injector",
    "version": "1.0",
    "description": "Extension to inject custom code per domain",
    "manifest_version": 2,
    "content_scripts": [
        {
            "matches": ["https://pro.coinbase.com/trade/BTC-USD"],
            "run_at": "document_start",
            "js": ["cbp-reddit.js"]
        }
    ]
}